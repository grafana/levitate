{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "target": "ES6",
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
  },
  "exclude": ["./dist", "./fixtures"],
  "files": ["./src/bin.ts"],
  "include": ["src/**/*"]
}





